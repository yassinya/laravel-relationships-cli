{
    "name": "yassinya/laravel-relation-cli",
    "description": "Quickly define relationships between eloquent models with a simple command",
    "type": "library",
    "license": "mit",
    "authors": [
        {
            "name": "Yassin Youssouf",
            "email": "yassinrsc@gmail.com"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/support": "~5.8.0|^6.0|^7.0",
        "illuminate/filesystem": "~5.8.0|^6.0|^7.0",
        "illuminate/console": "~5.8.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Yassinya\\Relation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yassinya\\Relation\\RelationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
